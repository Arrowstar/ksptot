# Hock & Schittkowski 100

var x{1..8};

minimize f:
	(x[1]-10)^2 +5*(x[2]-12)^2 + x[3]^4 + 3*(x[4]-11)^2 +
   10*x[5]^6 + 7*x[6]^2 +x[7]^4 - 4*x[6]*x[7] - 10*x[6] - 8*x[7];

c1: x[1] == x[8];

d1: 127 - 2*x[1]^2 - 3*x[2]^4 - x[3] - 4*x[4]^2 - 5*x[5] >= 0;

d2:  -4*x[1]^2 - x[2]^2 + 3*x[1]*x[2] - 2*x[3]^2 - 5*x[6] + 11*x[7] >= 0;

data;
var x :=
1 1
2 2
3 0
4 4
5 0
6 1
7 1
8 0
;
